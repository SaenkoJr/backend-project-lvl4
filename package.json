{
  "name": "taks-manager-saenkomm",
  "version": "0.0.1",
  "description": "4th hexlet project",
  "engines": {
    "node": ">=13"
  },
  "scripts": {
    "test": "jest",
    "build-configs": "babel ormconfig.js webpack.config.js --out-dir dist",
    "build-server": "NODE_ENV=production babel --extensions .ts --extensions .js server --out-dir dist/server --source-maps inline && npm run build-configs && cp -r server/views dist/server/views",
    "build-frontend": "NODE_ENV=production npx webpack -p",
    "build": "rm -rf dist && npm run build-frontend && npm run build-server",
    "start": "NODE_ENV=production node dist/server/bin/server.js",
    "develop": "npx nodemon --exec DEBUG=application:* npx babel-node server/bin/server.js",
    "typeorm": "npx babel-node ./node_modules/.bin/typeorm",
    "migration:run": "npm run typeorm migration:run",
    "migration:gen": "npm run typeorm migration:generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SaenkoJr/backend-project-lvl4.git"
  },
